    "@iconify-json/vscode-icons": "^1.1.32",
    "@nuxt/content": "^2.9.0",
    "@nuxt/ui": "^2.11.1",
    "@nuxthq/studio": "^1.0.5",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/plausible": "^0.2.4",
    "@nuxtseo/module": "2.0.0-beta.40",
    "@types/node": "^20.10.5",
    "@vueuse/core": "^10.7.1",
    "@vueuse/nuxt": "^10.7.1",
    "case-police": "^0.6.1",
    "consola": "^3.2.3",
    "eslint": "^8.56.0",
    "jiti": "^1.21.0",
    "magicast": "^0.3.2",
    "markdownlint-cli": "^0.38.0",
    "minisearch": "^6.3.0",
    "nuxt": "^3.9.0",
