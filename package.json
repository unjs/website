{
  "name": "website",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
